<idea-plugin>
    <id>me.jacobtread.v.plugin</id>
    <name>V Lang</name>
    <vendor>Jacobtread</vendor>
    <depends>com.intellij.modules.platform</depends>
    <extensions defaultExtensionNs="com.intellij">
        <fileType
                name="V File" implementationClass="me.jacobtread.v.plugin.VFileType"
                fieldName="INSTANCE" language="V" extensions="v"/>
        <moduleType id="VModule" implementationClass="me.jacobtread.v.plugin.module.VModuleType"/>
        <moduleBuilder builderClass="me.jacobtread.v.plugin.module.VModuleBuilder"/>
    </extensions>
</idea-plugin>
